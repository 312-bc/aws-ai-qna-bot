{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "lambda:CreateFunction",
                "cloudformation:ListExports",
                "s3:ListAccessPointsForObjectLambda",
                "cloudwatch:PutDashboard",
                "translate:GetTerminology",
                "kms:Decrypt",
                "firehose:*",
                "s3:PutStorageLensConfiguration",
                "cognito-identity:CreateIdentityPool",
                "iam:CreateRole",
                "dynamodb:*",
                "iam:AttachRolePolicy",
                "lambda:GetFunctionConfiguration",
                "iam:PutRolePolicy",
                "cognito-identity:GetOpenIdToken",
                "ssm:GetParameter",
                "cloudformation:ListTypeRegistrations",
                "ssm:RemoveTagsFromResource",
                "kms:Encrypt",
                "kms:GetKeyRotationStatus",
                "ssm:AddTagsToResource",
                "translate:ImportTerminology",
                "cognito-identity:DeleteIdentities",
                "translate:TranslateText",
                "cloudformation:CreateStackSet",
                "events:*",
                "cognito-identity:DescribeIdentity",
                "sns:*",
                "iam:GetRole",
                "cloudwatch:GetDashboard",
                "s3:PutAccountPublicAccessBlock",
                "apigateway:*",
                "lambda:InvokeFunction",
                "s3:ListJobs",
                "ssm:GetParameters",
                "ssm:PutParameter",
                "translate:ListTerminologies",
                "cognito-idp:*",
                "s3:CreateJob",
                "iam:GetRolePolicy",
                "cloudformation:ValidateTemplate",
                "cloudformation:ListTypeVersions",
                "cloudformation:CreateUploadBucket",
                "cloudformation:RegisterType",
                "cloudformation:DescribeStackDriftDetectionStatus",
                "cloudformation:SetTypeDefaultVersion",
                "s3:GetAccessPoint",
                "kms:PutKeyPolicy",
                "cloudformation:ListTypes",
                "cognito-identity:ListIdentityPools",
                "kms:ListResourceTags",
                "cloudformation:DeregisterType",
                "cloudformation:EstimateTemplateCost",
                "cognito-identity:GetId",
                "iam:PassRole",
                "cognito-identity:SetPrincipalTagAttributeMap",
                "cognito-identity:SetIdentityPoolRoles",
                "cloudformation:DescribeAccountLimits",
                "kms:DescribeKey",
                "kms:CreateKey",
                "ssm:GetParametersByPath",
                "kms:CreateGrant",
                "kms:EnableKeyRotation",
                "cloudformation:ListStacks",
                "cloudformation:DescribeType",
                "cloudformation:ListImports",
                "s3:ListAccessPoints",
                "ssm:GetParameterHistory",
                "lambda:GetFunction",
                "kms:GetKeyPolicy",
                "codecommit:GitPull",
                "cloudformation:DescribeTypeRegistration",
                "iam:CreatePolicy",
                "s3:ListStorageLensConfigurations",
                "kendra:*",
                "es:*",
                "iam:CreateServiceLinkedRole",
                "s3:GetAccountPublicAccessBlock",
                "lambda:AddPermission",
                "s3:ListAllMyBuckets",
                "cognito-identity:UnlinkIdentity",
                "cognito-identity:GetCredentialsForIdentity"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": "cognito-identity:*",
            "Resource": "arn:aws:cognito-identity:*:xxxx:identitypool/*"
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": "arn:aws:s3:::*/*"
        },
        {
            "Sid": "VisualEditor3",
            "Effect": "Allow",
            "Action": "cloudformation:*",
            "Resource": [
                "arn:aws:cloudformation:*:xxxx:stackset/*/*",
                "arn:aws:cloudformation:*:xxxx:stackset-target/*",
                "arn:aws:cloudformation:*:xxxx:stack/*/*",
                "arn:aws:cloudformation:*:xxxx:type/resource/*"
            ]
        },
        {
            "Sid": "VisualEditor4",
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::*",
                "arn:aws:s3:*:xxxx:accesspoint/*",
                "arn:aws:s3:*:xxxx:storage-lens/*",
                "arn:aws:s3:*:xxxx:job/*",
                "arn:aws:s3-object-lambda:*:xxxx:accesspoint/*"
            ]
        }
    ]
}